{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "verbose": true,
  "collectCoverage": true,
  "collectCoverageFrom": ["src/**/*.ts"],
  "coverageThreshold": {
    "global": {
      "branches": 40,
      "functions": 30,
      "lines": 50,
      "statements": 50
    }
  },
  "coverageReporters": ["clover", "json", "lcov", "text"]
}
