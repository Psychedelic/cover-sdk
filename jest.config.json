{
  "preset": "ts-jest",
  "verbose": true,
  "collectCoverage": true,
  "collectCoverageFrom": ["src/**/*.ts"],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  },
  "coverageReporters": ["clover", "json", "lcov", "text"]
}
